{
  "name": "@openmrs/esm-devtools",
  "description": "Dev tools for frontend devs using OpenMRSS",
  "main": "dist/openmrs-esm-devtools.js",
  "version": "1.1.0",
  "license": "MPL-2.0",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint . --ext tsx",
    "test": "jest --config jest.json",
    "watch-tests": "jest --config jest.json --watch",
    "typescript": "tsc",
    "prepublishOnly": "npm run build",
    "build": "webpack --mode production"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "directories": {
    "lib": "dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'npm:test' 'npm:typescript' 'npm:lint'"
    }
  },
  "types": "src/openmrs-esm-devtools.tsx",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@openmrs/esm-api": "1.0.3",
    "@openmrs/react-root-decorator": "^1.0.0",
    "@reach/tabs": "^0.1.6",
    "@types/jest": "^24.0.18",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.5",
    "browserslist-config-openmrs": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-ts-react-important-stuff": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "kremling": "^2.0.1",
    "lodash-es": "^4.17.15",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "semver": "^6.3.0",
    "single-spa-react": "^2.10.2",
    "style-loader": "^1.0.0",
    "systemjs-webpack-interop": "^1.1.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/single-spa-react": "^2.8.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
